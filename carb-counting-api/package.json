{
  "name": "carb-counting-api",
  "author": "Vinicius Santiago <viniciussdsilva@gmail.com>",
  "license": "MIT",
  "version": "0.0.0",
  "description": "Carb Counting API using Serverless",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "lint:fix": "eslint 'src/**' --fix",
    "pretest": "jest --clearCache",
    "test": "jest --passWithNoTests --no-cache --silent --noStackTrace --runInBand",
    "test:coverage": "npm test -- --coverage",
    "test:staged": "npm test -- --findRelatedTests",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:generate": "npm run typeorm -- migration:generate",
    "posttypeorm:generate": "npm run lint:fix",
    "typeorm:revert": "npm run typeorm -- migration:revert",
    "typeorm:run": "npm run typeorm -- migration:run"
  },
  "devDependencies": {
    "@serverless/typescript": "^2.23.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "faker": "^5.5.3",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "json-schema-to-ts": "^1.5.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "serverless": "^2.23.0",
    "serverless-webpack": "^5.3.5",
    "ts-jest": "^27.0.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.20.2",
    "webpack-node-externals": "^2.5.2"
  },
  "dependencies": {
    "pg": "^8.7.1",
    "presentation-common": "file:../libs/presentation-common",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^2.0.0"
  }
}
